{
	"source": "./src",
	"destination": "./esdoc",
	"plugins": [{
		"name": "esdoc2-standard-plugin",
		"option": {
			"manual": {
				"files": [
					"./doc/overview.md",
					"./doc/getting-startet.md",
					"./doc/configuration.md",
					"./CHANGELOG.md"
				]
			},
			"test": {
				"source": "./test"
			}
		}
	}, {
		"name": "esdoc2-ecmascript-proposal-plugin",
		"option": {
			"classProperties": true,
			"objectRestSpread": true,
			"doExpressions": true,
			"functionBind": true,
			"functionSent": true,
			"asyncGenerators": true,
			"decorators": true,
			"exportExtensions": true,
			"dynamicImport": true
		}
	}, {
		"name": "esdoc2-coverage-plugin"
	}]
}
